{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp", "--headless"]
    }
  },
  "permissions": {
    "allow": [
      "mcp__playwright__*",
      "Bash(curl:*)",
      "Bash(node:*)",
      "Read",
      "Write"
    ],
    "deny": []
  },
  "hasCompletedOnboarding": true
}
